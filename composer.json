{
	"name": "stinger-soft/aggrid-bundle",
	"type": "symfony-bundle",
	"description": "Abstraction Layer to create data tables with AgGrid https://www.ag-grid.com/",
	"keywords": [
		"Grid",
		"Table"
	],
	"homepage": "http://www.stinger-soft.net",
	"authors": [
		{
			"name": "Oliver Kotte",
			"email": "oliver.kotte@stinger-soft.net",
			"role": "Developer"
		},
		{
			"name": "Florian Meyer",
			"email": "florian.meyer@stinger-soft.net",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"StingerSoft\\AggridBundle\\": ""
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"license": "Apache-2.0",
	"require": {
		"php": "^8.1",
		"ext-json": "*",
		"knplabs/knp-components": "^3.0||^4.0||^5.0",
		"stinger-soft/php-commons": "^1.0.0",
		"symfony/framework-bundle": "^5.4||^6.4",
		"symfony/property-access": "^5.4||^6.4",
		"symfony/console": "^5.4||^6.4",
		"symfony/form": "^5.4||^6.4",
		"twig/twig": "^2||^3.0"
	},
	"suggest": {
		"npm-asset/ag-grid-enterprise": "Enterprise edition to support set filter etc."
	},
	"conflict": {
	  "npm-asset/ag-grid-enterprise": "21.2.0"
	},
	"require-dev": {
		"foxy/foxy":"^1.0.0",
		"php-coveralls/php-coveralls": "^2.1.0",
		"doctrine/orm": "^3",
		"doctrine/common": "^3",
		"symfony/validator": "^6.4",
		"symfony/phpunit-bridge": "^6.4",
		"symfony/test-pack": "*"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"foxy": true,
		"branch-alias": {
			"dev-master": "2.2.x-dev",
			"dev-develop": "3.0.x-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"foxy/foxy": true
		}
	}
}
