{
	"name": "stinger-soft/aggrid-bundle",
	"type": "symfony-bundle",
	"description": "Abstraction Layer to create data tables with AgGrid https://www.ag-grid.com/",
	"keywords": [
		"Grid",
		"Table"
	],
	"homepage": "http://www.stinger-soft.net",
	"authors": [
		{
			"name": "Oliver Kotte",
			"email": "oliver.kotte@stinger-soft.net",
			"role": "Developer"
		},
		{
			"name": "Florian Meyer",
			"email": "florian.meyer@stinger-soft.net",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"StingerSoft\\AggridBundle\\": ""
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"license": "Apache-2.0",
	"require": {
		"php": ">=7.2",
		"ext-json": "*",
		"npm-asset/ag-grid-community": "~21.0",
		"npm-asset/moment": "~2.24",
		"symfony/framework-bundle": "^4.3",
		"symfony/property-access": "^4.3",
		"symfony/console": "|^4.3",
		"symfony/form": "^4.3",
		"knplabs/knp-components": "~1.3.3|^2.0",
		"stinger-soft/php-commons": "^1.0.0"
	},
	"suggest": {
		"npm-asset/ag-grid-enterprise": "Enterprise edition to support set filter etc."
	},
	"conflict": {
	  "npm-asset/ag-grid-enterprise": "21.2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.5.6",
		"php-coveralls/php-coveralls": "~2.1.0",
		"doctrine/orm": "^2.5",
		"doctrine/common": "^2.5",
		"symfony/validator": "^3.4|^4.0",
		"symfony/phpunit-bridge": "^3.4|^4.0"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev",
			"dev-feature/webpack": "2.0.x-dev"
		}
	}
}
