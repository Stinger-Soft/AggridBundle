{% extends '@StingerSoftAggrid/Column/column.js.twig' %}
{% block column_def_start %}
dateFormat: {{ column.vars.date_format|json_encode|raw }},
cellEditor: 'datePicker',
comparator: function(dateA, dateB) {
    if (dateA == dateB) {
        return 0;
    }
    if(typeof dateB === 'undefined' || dateB === null) {
        return 1;
    }
    if(typeof dateA === 'undefined' || dateA === null) {
        return -1;
    }

    if (dateA.date == dateB.date) {
        return 0;
    }

    if (dateA.date < dateB.date) {
        return -1;
    }

    if (dateA.date > dateB.date) {
        return 1;
    }
},
{% endblock column_def_start %}